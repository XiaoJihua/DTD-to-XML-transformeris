<xsd:sequence minOccurs="1" maxOccurs="1">
<xsd:element name="elem5" type="xsd:string" minOccurs="1" maxOccurs="1"/>
<xsd:element name="elem6" type="xsd:string" minOccurs="1" maxOccurs="1"/>
</xsd:sequence>
</xsd:choice>
<xsd:choice minOccurs="1" maxOccurs="1">
<xsd:element name="elem7" type="xsd:string" minOccurs="1" maxOccurs="1"/>
