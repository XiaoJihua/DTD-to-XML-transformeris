<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
<xsd:element name="team">
<xsd:complexType>
<xsd:sequence minOccurs="0" maxOccurs="unbounded">
<xsd:element name="person" minOccurs="1" maxOccurs="1">
<xsd:complexType>
<xsd:sequence minOccurs="1" maxOccurs="1">
<xsd:element name="name" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
<xsd:element name="city" type="xsd:string" minOccurs="0" maxOccurs="1"/>
<xsd:choice minOccurs="1" maxOccurs="1">
<xsd:sequence minOccurs="1" maxOccurs="1">
<xsd:element name="elem1" type="xsd:string" minOccurs="1" maxOccurs="1"/>
<xsd:sequence minOccurs="1" maxOccurs="1">
<xsd:element name="elem2" type="xsd:string" minOccurs="1" maxOccurs="1"/>
<xsd:element name="elem3" type="xsd:string" minOccurs="1" maxOccurs="1"/>
</xsd:sequence>
<xsd:element name="elem4" type="xsd:string" minOccurs="1" maxOccurs="1"/>
